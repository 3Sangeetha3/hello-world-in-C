#include<stdio.h>
//#include<math.h>
int main()
{
	int n1, n2, n3;
	printf("enter the value of n1, n2, n3: ");
	scanf("%d %d %d",&n1,&n2,&n3);
	
	if(n1>n2 & n1>n3){
		printf(" larger number is: %d",n1);
	}
	else if(n2>n3 & n2>n1){
		printf(" larger number is: %d",n2);
	}
	else if(n3>n1 & n3>n2){
		printf("larger number is: %d",n3);
	}
	else if(n1 == n2 & n1 > n3){
		printf("Two numbers are same and larger num is :%d",n1);
	}
	else if(n1 == n3 & n1 > n2){
		printf("Two numbers are same and larger num is :%d",n1);
	}
	else if(n2 == n3 & n2 > n1){
		printf("Two numbers are same and larger num is : %d",n2);
	}
	else
		printf("All numbers are same");

	return 0;
}

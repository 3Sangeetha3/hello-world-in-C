#include<stdio.h>

int main()
{
    int a=10 , b=20;
    printf("greater than operator; %d",a>b);
    printf("\nless than operator: %d",a<b);
		
    printf("\ngreater than or equl operator: %d",a>=b); //false=0,true=1
    printf("\nless or equal to operator:%d",a<=b);
		
    printf("\nEqual to operator:%d",a==b); // Equality check
    printf("\nnot equal to operator:%d",a!=b);
		
    a=30,b=30;
    printf("equality check:%d",a==b);


}
